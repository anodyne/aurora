; EditorConfig: http://EditorConfig.org

; top-most EditorConfig file
root = true

; Unix-style newlines, no newline ending files, tab indents, utf-8
[*]
end_of_line = lf
insert_final_newline = false
indent_style = tab
charset = utf-8

[node_modules/**]
indent_style = ignore
charset = ignore
end_of_line = ignore

[vendor/**]
indent_style = ignore
charset = ignore
end_of_line = ignore
