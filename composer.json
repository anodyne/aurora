{
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.4.*",
        "laracasts/presenter": "*",
        "nothingworks/blade-svg": "~0.1"
    },

    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "~2.8"
    },

    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },

    "autoload-dev": {
        "files": [
            "tests/utilities/functions.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
